{
  "files": [
    {
      "path": "out/_next/static/js/*.js",
      "maxSize": "400kb",
      "compression": "gzip"
    },
    {
      "path": "out/_next/static/css/*.css",
      "maxSize": "50kb",
      "compression": "gzip"
    },
    {
      "path": "out/index.html",
      "maxSize": "30kb",
      "compression": "none"
    }
  ],
  "defaultCompression": "gzip",
  "ci": {
    "trackBranches": ["main", "master"],
    "repoBranchBase": "main"
  }
}